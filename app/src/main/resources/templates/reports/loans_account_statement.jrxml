	<dataset name="transactions" uuid="96455207-6f45-483c-b991-1afee43f31e1">
		<property name="com.jaspersoft.studio.data.defaultdataadapter" value="finance_loans_module"/>
		<property name="com.jaspersoft.studio.data.sql.tables" value=""/>
		<parameter name="forceNumber" class="java.lang.String"/>
		<parameter name="currency" class="java.lang.String"/>
		<parameter name="runby" class="java.lang.String"/>
		<parameter name="startDate" class="java.lang.String"/>
		<parameter name="loanType" class="java.lang.String"/>
		<query language="sql"><![CDATA[WITH AccountInfo AS (
    SELECT
        la.id AS account_id,
        l.loan_id AS loan_id,
        l.loan_type
    FROM loan_account la
             JOIN loan l ON l.loan_account_id = la.id
    WHERE la.force_number = $P{forceNumber}
      AND l.currency_id = $P{currency}
      AND l.loan_type = $P{loanType}
),
     AllTransactions AS (
         SELECT
             t.force_number AS forceNumber,
             t.date_posted AS datePosted,
             t.transaction_date AS transactionDate,
             t.transaction_code,
             tt.name,
             ai.loan_type,
             CASE
                 WHEN ai.loan_type = 'PROJECT' AND tt.project_account_effect = 'DR' THEN t.amount
                 WHEN ai.loan_type = 'LOAN' AND tt.loans_account_effect = 'DR' THEN t.amount
                 ELSE 0
                 END AS DR,
             CASE
                 WHEN ai.loan_type = 'PROJECT' AND tt.project_account_effect = 'CR' THEN t.amount
                 WHEN ai.loan_type = 'LOAN' AND tt.loans_account_effect = 'CR' THEN t.amount
                 ELSE 0
                 END AS CR,
             SUM(
                     CASE
                         WHEN ai.loan_type = 'PROJECT' AND tt.project_account_effect = 'DR' THEN t.amount
                         WHEN ai.loan_type = 'PROJECT' AND tt.project_account_effect = 'CR' THEN -t.amount
                         WHEN ai.loan_type = 'LOAN' AND tt.loans_account_effect = 'DR' THEN t.amount
                         WHEN ai.loan_type = 'LOAN' AND tt.loans_account_effect = 'CR' THEN -t.amount
                         ELSE 0
                         END
             ) OVER (
                         ORDER BY t.transaction_date, t.date_posted
                         ROWS UNBOUNDED PRECEDING
                         ) AS running_balance
         FROM transactions t
                  JOIN [zdf-bf-system-parameters].dbo.transaction_type tt ON t.transaction_code = tt.code
                  JOIN AccountInfo ai ON t.reference_id = ai.loan_id
     )
SELECT
    at.datePosted,
    at.transactionDate,
    at.transaction_code,
    at.name AS transaction_name,
    at.DR,
    at.CR,
    COALESCE((
                 SELECT TOP 1 running_balance
                 FROM AllTransactions
                 WHERE datePosted < $P{startDate}
                 ORDER BY transactionDate DESC, datePosted DESC
             ), 0) AS opening_balance,
    at.running_balance
FROM AllTransactions at
WHERE $P{startDate} IS NULL OR at.datePosted >= $P{startDate}
ORDER BY at.transactionDate, at.datePosted;
]]></query>
