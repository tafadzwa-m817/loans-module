	<query language="sql"><![CDATA[SELECT
    la.loan_account_number AS account_number,
    la.id AS account_id,
    m.first_name AS name,
    m.last_name AS surname,
    la.force_number,
    (SELECT rank_name
     FROM [zdf-bf-system-parameters].dbo.ranks
     WHERE id = m.rankid) AS rank,
    (SELECT currency_name
     FROM [zdf-bf-system-parameters].dbo.currencies
     WHERE id = $P{currency}) AS currency_name,
    l.balance,
    l.loan_id,
    ISNULL(SUM(l.balance) OVER (PARTITION BY la.force_number), 0.0) AS current_balance
FROM
    loan_account la
        JOIN
    member m ON m.force_number = la.force_number
        JOIN
    loan l ON l.loan_account_id = la.id
WHERE
    la.force_number = $P{forceNumber}
  AND l.currency_id = $P{currency}
  AND l.loan_type = $P{loanType}]]></query>
