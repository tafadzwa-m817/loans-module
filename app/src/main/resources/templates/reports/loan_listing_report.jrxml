	<query language="sql"><![CDATA[SELECT
    la.force_number AS forceNumber,
    la.loan_account_number AS loanAccountNumber,
    l.first_name AS firstName,
    l.last_name AS lastName,
    l.balance AS balance,
    l.loan_advance as loan_amount,
    r.rank_name AS rank,
    (SELECT currency_name
     FROM [zdf-bf-system-parameters].dbo.currencies
     WHERE id = $P{currency}) AS currency_name

FROM
    loan_account la
        JOIN
    loan l ON la.id = l.loan_account_id
        JOIN
    [zdf-bf-system-parameters].dbo.ranks r ON l.rank_id = r.id
WHERE l.loan_type = $P{category}
    AND l.currency_id = $P{currency};
]]></query>
