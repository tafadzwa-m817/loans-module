	<query language="sql"><![CDATA[select force_number,
       claim_type,
       claim_status,
       (account_holder_name+ ' ' + account_holder_surname) AS member,
       (claimant_name + ' ' + claimant_surname) AS claimant,
       claim_amount,
       (SELECT currency_name 
       FROM [zdf-bf-system-parameters].dbo.currencies
       WHERE id = currency_id) AS currency_name
from claim
where
    (claim_type = $P{claim_type} OR $P{claim_type} IS NULL)
and (claim_status = $P{claim_status} OR $P{claim_status} IS NULL)
]]></query>
