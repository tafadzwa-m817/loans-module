	<query language="sql"><![CDATA[select cl.force_number,
       claim_status,
       (account_holder_name+ ' ' + account_holder_surname) AS member,
       (claimant_name + ' ' + claimant_surname) AS claimant,
       claim_type,
       rate,
       claimant_id_number,
       (SELECT rank_name
        FROM [zdf-bf-system-parameters].dbo.ranks
        WHERE id = m.rankid) AS rank,
       (SELECT currency_name
        FROM [zdf-bf-system-parameters].dbo.currencies
        WHERE id = cl.currency_id) AS currency_name,
    loan_securities_local_balance,
    loan_securities_usd_balance,
    project_securities_local_balance,
    project_securities_usd_balance,
    -- show from here
    loan_project_securities_balance,
    loan_usd_balance,
    loan_local_balance,
    project_usd_balance,
    project_local_balance,
    total_loan_project_balance,
    subscriptionusdbalance,
    subscription_total_balance,
    subscription_local_currency_balance,
    total_owing,
    claim_amount
from claim cl
         JOIN
     member m ON m.force_number = cl.force_number
where
    cl.id = $P{claim_id}]]></query>
