	<parameter name="runby" class="java.lang.String"/>
	<parameter name="currency" class="java.lang.String">
		<defaultValueExpression><![CDATA[$P{currency}]]></defaultValueExpression>
	</parameter>
	<parameter name="status" class="java.lang.String"/>
	<query language="sql"><![CDATA[
SELECT
    p.serial_number,
    p.project_name,
    prov.name AS province_name,
    t.name AS town_name,
    p.developer,
    p.category_per_sqm AS category,
    p.total_area_in_sqm,
    p.number_of_stands,
    p.status,
    p.date_of_purchase,
    (SELECT currency_name
     FROM [zdf-bf-system-parameters].dbo.currencies
     WHERE id = $P{currency}) AS currency_name
FROM
    project p
        JOIN
    [zdf-bf-system-parameters].dbo.province prov ON p.province_id = prov.id
        JOIN
    [zdf-bf-system-parameters].dbo.town t ON p.town_id = t.id
WHERE
	(p.status = $P{status} OR $P{status} IS NULL)
	AND prov.is_active = 'TRUE' AND prov.is_deleted = 'FALSE'
    AND t.is_active = 'TRUE' AND t.is_deleted = 'FALSE'
    AND p.is_deleted = 'FALSE';
]]></query>
